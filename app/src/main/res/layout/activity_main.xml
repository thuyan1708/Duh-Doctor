<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="0dp"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="-1dp">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="117dp"
        android:layout_height="111dp"
        app:srcCompat="@drawable/pic_1"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="521dp" />

    <View
        android:id="@+id/blueBox2"
        android:layout_width="412dp"
        android:layout_height="443dp"
        android:background="@drawable/rounded_square"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="298dp"
        android:layout_height="52dp"
        android:layout_centerHorizontal="true"
        android:text="Quickly set-up and support appointment with your doctor!"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="458dp" />

    <View
        android:id="@+id/whiteBox2"
        android:layout_width="335dp"
        android:layout_height="161dp"
        android:layout_marginTop="232dp"
        android:background="@drawable/square"
        app:layout_constraintHorizontal_bias="0.581"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="57dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/final_logo"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="259dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnSetAppointment"
        android:layout_width="264dp"
        android:layout_height="44dp"
        android:layout_below="@id/tvWelcome"
        android:backgroundTint="#2C87FF"
        android:text="SET APPOINTMENT"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="259dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnViewAppointment"
        android:layout_width="263dp"
        android:layout_height="46dp"
        android:layout_below="@id/btnSetAppointment"
        android:backgroundTint="#2C87FF"
        android:text="VIEW APPOINTMENT"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="320dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="58dp"
        android:layout_height="45dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="320dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvWelcome"
        android:layout_width="218dp"
        android:layout_height="156dp"
        android:layout_centerHorizontal="true"
        android:text="Welcome to Duh-Doctor App!"
        android:textColor="#FBF6F6"
        android:textSize="32sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="63dp"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/btnLogout"
        android:layout_width="67dp"
        android:layout_height="74dp"
        android:layout_below="@id/btnViewAppointment"
        android:backgroundTint="#9DADC1"

        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="258dp"
        android:layout_height="220dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pic_corner"
        tools:layout_editor_absoluteX="193dp" />

    <Button
        android:id="@+id/btnSetAppointment"
        android:layout_width="67dp"
        android:layout_height="74dp"
        android:backgroundTint="#2C87FF"
        tools:layout_editor_absoluteX="319dp"
        tools:layout_editor_absoluteY="632dp"
        tools:ignore="DuplicateIds,MissingConstraints" />

    <ImageView
        android:id="@+id/imageSetAppointment"
        android:layout_width="33dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/note"
        tools:layout_editor_absoluteX="336dp"
        tools:layout_editor_absoluteY="651dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="41dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/out_logo"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="648dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>